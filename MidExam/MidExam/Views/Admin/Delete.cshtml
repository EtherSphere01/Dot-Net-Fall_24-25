
@{
    ViewBag.Title = "Delete";
}

<h2>Delete Products</h2>
<form method="post">
<table>
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
        <th>Quantity</th>
        <th>Reorder Level</th>
    </tr>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.Name</td>
        <td>@Model.Quantity</td>
        <td>@Model.Reorder</td>
    </tr>

</table>
<input type="submit" value="Yes" class="btn btn-danger" name="dscn" />
<input type="submit" value="No" class="btn btn-warning" name="dscn" />
</form>

